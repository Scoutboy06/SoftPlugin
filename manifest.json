{
	"manifest_version": 3,
	"name": "SoftPlugin - SchoolSoft Addon",
	"version": "1.0.6",
	"description": "SchoolSoft did it first, we did it better.",
	"action": {
		"default_icon": "assets/icon_128.png",
		"default_popup": "popup/popup.html"
	},
	"permissions": ["storage"],
	"content_scripts": [
		{
			"js": ["jquery.min.js", "content/content.js"],
			"css": ["content/content.css", "content/darkmode.css"],
			"matches": ["https://sms.schoolsoft.se/*"]
		},
		{
			"matches": ["https://login.grandid.com/*"],
			"js": ["jquery.min.js", "content/autologin.js"]
		}
	],
	"icons": {
		"16": "assets/icon_16.png",
		"48": "assets/icon_48.png",
		"128": "assets/icon_128.png"
	}
}
